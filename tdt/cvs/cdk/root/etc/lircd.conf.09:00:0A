#
# remote control file for SPARK boxes
# only the default entry is tested!
#
# key codes need to be valid KEY_xxx names
# (see neutrino rcinput code and linux/input.h)
#

begin remote

  name  rc09(Alien)
  bits           16
  flags SPACE_ENC|CONST_LENGTH
  eps            30
  aeps          100

  header       9074  4344
  one           694  1532
  zero          694   417
  ptrail        674
  repeat       9074  2105
  pre_data_bits   16
  pre_data       0x9966
  gap          107250
  toggle_bit_mask 0x0

      begin codes
          KEY_POWER                0xDA25
          KEY_MODE		   0x52AD # V.FORMAT
          KEY_TIME                 0x728D
          KEY_MUTE                 0x7A85
          PICASA                   0x1AE5
          SHOUTCAST                0x12ED
          YOUTUBE                  0x32CD
          SPARK                    0x3AC5
          KEY_F1                   0x827D
          KEY_F2                   0x00FF
          KEY_F3                   0xC03F
          KEY_F4                   0x40BF
          TV/SAT                   0x5AA5
          KEY_1                    0x4AB5
          KEY_2                    0x6A95
          KEY_3                    0x42BD
          KEY_4                    0x0AF5
          KEY_5                    0x2AD5
          KEY_6                    0x02FD
          KEY_7                    0xCA35
          KEY_8                    0xEA15
          KEY_9                    0xC23D
          KEY_TV                   0x8877  # TV/RADIO
          KEY_0                    0xA857
          KEY_PREVIOUS             0x807F  #Recall
          KEY_FIND                 0xB240
          KEY_REC                  0x708F
          KEY_VOLUMEUP             0x22DD
          KEY_VOLUMEDOWN           0x38C7
          KEY_PAGEUP               0xA05F
          KEY_PAGEDOWN             0xF807
          KEY_SAT                  0x629D
          KEY_FAVORITES            0xBA45
          KEY_MENU                 0x9A65
          KEY_INFO                 0xE21D
          KEY_EXIT                 0x58A7
          KEY_EPG                  0x7887
          KEY_UP                   0xD827
          KEY_DOWN                 0xF00F
          KEY_LEFT                 0x926D
          KEY_RIGHT                0x50AF
          KEY_OK                   0xD02F
          KEY_LIST                 0xA25D #FILELIST
          KEY_STOP                 0x609F
          KEY_PAUSE                0xE817
          KEY_PLAY                 0x28D7
          KEY_REWIND               0xAA55
          KEY_FORWARD              0x6897
          PLAY_MODE                0x20DF
          USB                      0xE01F
          KEY_RED                  0x8A75
          KEY_GREEN                0x08F7
          KEY_YELLOW               0xC837
          KEY_BLUE                 0x48B7
      end codes

end remote

